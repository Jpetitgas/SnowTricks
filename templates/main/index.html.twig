{% extends 'base.html.twig' %}

{% block title %}accueil
{% endblock %}
{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" href="{{ asset('/css/imageImbriquee.css')}}">

{% endblock %}

{% block body %}
	<div id="image" class="row texte-center">
		<div  class='product-holder'>
			<img src="\images\main_1.jpg" class="img-fluid" alt="Responsive image" class='product-image'>>
		</div>
	</div>
<div class="row justify-content-end">
	<button id="togg1">
		<i class="fa fa-chevron-down" aria-hidden="true" class='plus-image'></i>
	</button>
</div>

	<div id="content" class="container">
		{% include "main/_content.html.twig"%}
	</div>
</div>
<div class="row justify-content-end">
	<button id="togg2">
		<i class="fa fa-chevron-up" aria-hidden="true"></i>
	</button>
</div>

<div id="page">2</div>
<div class="row justify-content-center">
<button id="plus">+ de figures</button>
</div>


{% endblock %}
{% block javascripts %}
	<script src="{{ asset('/js/figure.js')}}"></script>
	{% endblock %}
